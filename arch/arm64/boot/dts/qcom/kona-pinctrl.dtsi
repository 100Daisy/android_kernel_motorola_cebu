&soc {
	tlmm: pinctrl@0F000000 {
		compatible = "qcom,kona-pinctrl";
		reg = <0x0F000000 0xC00000>;
		interrupts = <0 208 0>;
		gpio-controller;
		#gpio-cells = <2>;
		interrupt-controller;
		#interrupt-cells = <2>;
	};
};
